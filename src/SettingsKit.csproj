<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <RootNamespace>AluminiumTech.DevKit.SettingsKit</RootNamespace>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>SettingsKit</Title>
        <Authors>AluminiumTech</Authors>
        <Copyright>Copyright (c) AluminiumTech 2021-2022</Copyright>
        <RepositoryUrl>https://gitlab.com/aluminiumtechdevkit/SettingsKit/</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>
        <PackageReleaseNotes></PackageReleaseNotes>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
      <AssemblyName>SettingsKit</AssemblyName>
        <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
        <Description>A library to make managing JSON based Settings files easier for .NET .</Description>
        <Version>3.0.0</Version>
      <PackageVersion>4.0.0</PackageVersion>
        <Product>SettingsKit</Product>
        <LangVersion>latest</LangVersion>
        <AssemblyVersion>3.0.0.0</AssemblyVersion>
        <FileVersion>3.0.0.0</FileVersion>
        <TargetFramework>net6.0</TargetFramework>
    </PropertyGroup>

    <ItemGroup>
      <None Include="..\LICENSE.md">
        <Pack>True</Pack>
        <PackagePath></PackagePath>
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
    </ItemGroup>

</Project>
